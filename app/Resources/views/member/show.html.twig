{% extends 'base.html.twig' %}

{% block body %}
    <h1>Member</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ member.id }}</td>
            </tr>
            <tr>
                <th>Typ</th>
                <td>{{ member.typ }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ member.name }}</td>
            </tr>
            <tr>
                <th>Nameurl</th>
                <td>{{ member.nameUrl }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ member.password }}</td>
            </tr>
            <tr>
                <th>Descriptionshort</th>
                <td>{{ member.descriptionShort }}</td>
            </tr>
            <tr>
                <th>Descriptionlong</th>
                <td>{{ member.descriptionLong }}</td>
            </tr>
            <tr>
                <th>Descriptionmember</th>
                <td>{{ member.descriptionMember }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ member.image }}</td>
            </tr>
            <tr>
                <th>Kontakt</th>
                <td>{{ member.kontakt }}</td>
            </tr>
            <tr>
                <th>Kontaktemail</th>
                <td>{{ member.kontaktEmail }}</td>
            </tr>
            <tr>
                <th>Urlmyspace</th>
                <td>{{ member.urlMyspace }}</td>
            </tr>
            <tr>
                <th>Urlflickr</th>
                <td>{{ member.urlFlickr }}</td>
            </tr>
            <tr>
                <th>Urlpurevolume</th>
                <td>{{ member.urlPurevolume }}</td>
            </tr>
            <tr>
                <th>Urllastfm</th>
                <td>{{ member.urlLastfm }}</td>
            </tr>
            <tr>
                <th>Urlyoutube</th>
                <td>{{ member.urlYoutube }}</td>
            </tr>
            <tr>
                <th>Urlwebsite</th>
                <td>{{ member.urlWebsite }}</td>
            </tr>
            <tr>
                <th>Urlskype</th>
                <td>{{ member.urlSkype }}</td>
            </tr>
            <tr>
                <th>Urlicq</th>
                <td>{{ member.urlIcq }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if member.createdAt %}{{ member.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if member.updatedAt %}{{ member.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('member_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('member_edit', { 'id': member.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
